    <a href="/admin/dashboard">Dashboard</a>
    <table style="width: 50%;">
        <tr>
            <td>
                <img src="/userImage/<%= admin.image  %>" width="100px" height="100px" style="border-radius: 50%;" alt="<%= admin.image  %>">
            </td>
        </tr>
        <tr>
            <td>
                Name
            </td>
            <td>
                <%= admin.name  %>
            </td>
        </tr>
        <tr>
            <td>
